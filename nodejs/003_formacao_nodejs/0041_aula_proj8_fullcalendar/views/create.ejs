<%- include('partials/header.ejs') %>
<!-- =======================================-->


<h1>Cadastro de consulta</h1>
<hr>
<form action="/cadastro/create" method="post">
    <input type="text" name="name" id="name" class="form-control" id="name" placeholder="Nome do paciente">
    <input type="email" name="email" id="email" placeholder="email@email.com.br" class="form-control">
    <input type="text" name="cpf" id="cpf" placeholder="cpf" class="form-control">
    <input type="text" name="description" id="description" placeholder="descricao" class="form-control">
    <input type="date" name="date" id="date" placeholder="12/02/2019" class="form-control">
    <input type="time" name="time" id="tinme" placeholder="12/02/2019" class="form-control">
    <button class="btn btn-success btn-block">Agendar</button>
</form>







<!-- =======================================-->
<%- include('partials/footer.ejs') %>

<script>
    $("#cpf").mask( '000.000.000-00', { reverse: true } );
</script>




