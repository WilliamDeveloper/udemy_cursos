<table class="table">
    <thead>
        <tr>
            {{#with sqlResult}}
                {{#each nomeColunas as |coluna|}}
                        <th scope="col">{{this}}</th>
                {{/each}}
            {{/with}}
        </tr>
    </thead>
    <tbody>
        {{#with sqlResult}}
            {{#each rows as |row|}}
                <tr>
                    {{#each row}}
                        <td scope="row">{{this}}</td>
                    {{/each}}
                </tr>
            {{/each}}
        {{/with}}

    </tbody>
</table>